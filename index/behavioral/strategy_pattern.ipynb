    "class EmpathyStrategy(ResponseStrategy):\n",
    "    \"\"\"Strategy for handling negative emotions - Focus on understanding and support\"\"\"\n",
    "    \n",
    "    def __init__(self):\n",
    "        self.client = MockAIClient(\"empathy-model\")\n",

    "    \n",
    "    def get_strategy_info(self) -> Dict[str, Any]:\n",
    "        return {\n",
    "            \"name\": self.name,\n",
    "            \"tone\": \"ğŸ¤ Empathetic\",\n",
    "            \"approach\": \"ğŸ’™ Understanding & Support\",\n",
    "            \"best_for\": \"Angry, frustrated, disappointed users\",\n",
    "            \"key_phrases\": [\"I understand\", \"I'm sorry\", \"Let me help\", \"Your concern is valid\"]\n",
